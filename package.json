{
  "name": "redwar",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "egg": {
    "typescript": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=redwar",
    "stop": "egg-scripts stop --title=redwar",
    "dev": "egg-bin dev -r egg-ts-helper/register",
    "debug": "egg-bin debug -r egg-ts-helper/register",
    "test-local": "egg-bin test -r egg-ts-helper/register",
    "test": "npm run lint -- && npm run test-local",
    "cov": "egg-bin cov -r egg-ts-helper/register",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "lint": "tslint --project . -c tslint.json --fix",
    "prettier": "prettier-tslint fix '**/*.ts'",
    "clean": "ets clean",
    "yarn": "yarn",
    "yarn-clean": "rm -rf node_modules"
  },
  "dependencies": {
    "@types/egg-mock": "^3.0.4",
    "@types/ioredis": "^4.0.4",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/supertest": "^2.0.0",
    "apidoc": "^0.17.7",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "bluebird": "^3.5.3",
    "decimal102": "^0.0.2",
    "egg": "^2.6.1",
    "egg-cache": "^0.0.5",
    "egg-cors": "^2.1.1",
    "egg-grpc": "^1.0.3",
    "egg-grpc-server": "^1.7.0",
    "egg-json-logger": "^1.1.5",
    "egg-redis": "^2.0.0",
    "egg-scripts": "^2.6.0",
    "egg-sequelize": "^4.2.0",
    "egg-session": "^3.1.0",
    "egg-session-redis": "^2.1.0",
    "egg-ts-helper": "^1.11.0",
    "egg-validate": "^2.0.2",
    "egg-view-nunjucks": "^2.2.0",
    "grpc": "^1.17.0",
    "ioredis": "^4.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "pg": "^7.6.1",
    "request": "^2.88.0",
    "sequelize": "^4.41.2",
    "tslib": "^1.9.0",
    "typescript": "^3.0.0",
    "xlsx": "^0.14.1"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "egg-bin": "^4.6.2",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "prettier": "^1.16.4",
    "prettier-tslint": "^0.4.2",
    "tslint": "^5.12.1",
    "tslint-config-egg": "^1.0.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
